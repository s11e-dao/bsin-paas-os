  {
    "rule": "decision_rule_1_11",
    "attributes": {
      "lock-on-active": true
    },
    "when": {
      "conditions": [
        {
          "type": "Map",
          "variable": "$map"
        },
        {
          "type": "Object",
          "conditions": [
            {
              "expression": "$map['sex'] == '女'"
            },
            {
              "expression": "$map['userAge'] > '20'",
              "and": "$map['userName'] == '张三'"
            }
          ]
        }
      ]
    },
    "then": [
      {
        "action": "globalMap.put",
        "key": "userScore",
        "value": "100"
      },
      {
        "action": "globalMap.put",
        "key": "validVip",
        "value": "不是vip"
      },
      {
        "action": "modify",
        "object": "$map",
        "changes": {
          "userScore": "100",
          "validVip": "不是vip"
        }
      },
      {
        "action": "System.out.println",
        "message": "触发规则：decision_rule_1_11"
      }
    ]
  },
  {
    "rule": "decision_rule_1_12",
    "attributes": {
      "lock-on-active": true
    },
    "when": {
      "conditions": [
        {
          "type": "Map",
          "variable": "$map"
        },
        {
          "type": "Object",
          "conditions": [
            {
              "expression": "$map['sex'] == '男'"
            },
            {
              "expression": "$map['userAge'] <= '20'"
            }
          ]
        }
      ]
    },
    "then": [
      {
        "action": "globalMap.put",
        "key": "userScore",
        "value": "500"
      },
      {
        "action": "globalMap.put",
        "key": "validResult",
        "value": "通过"
      },
      {
        "action": "modify",
        "object": "$map",
        "changes": {
          "userScore": "500",
          "validResult": "通过"
        }
      },
      {
        "action": "System.out.println",
        "message": "触发规则：decision_rule_1_12"
      }
    ]
  },
  {
    "rule": "decision_rule_1_13",
    "attributes": {
      "lock-on-active": true
    },
    "when": {
      "conditions": [
        {
          "type": "Map",
          "variable": "$map"
        },
        {
          "type": "Object",
          "conditions": [
            {
              "expression": "!($map['sex'] == '男' && $map['userAge'] <= '20')"
            },
            {
              "expression": "$map['userName'] == '李四'"
            }
          ]
        }
      ]
    },
    "then": [
      {
        "action": "globalMap.put",
        "key": "userScore",
        "value": "300"
      },
      {
        "action": "modify",
        "object": "$map",
        "changes": {
          "userScore": "300"
        }
      },
      {
        "action": "System.out.println",
        "message": "触发规则：decision_rule_1_13"
      }
    ]
  }
